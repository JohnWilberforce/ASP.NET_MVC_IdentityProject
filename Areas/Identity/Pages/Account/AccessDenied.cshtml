@page
@using IdentityFromScratch.Policy
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}

<header>
    <h1 class="text-danger">@ViewData["Title"]</h1>
    <p class="text-danger">You do not have access to this resource.</p>
</header>
<br />
<br />
@{

    var c = Request.Cookies["AuthenticationFailureReason"];
    if (c != null)
    {
        <text>@c</text>
        Response.Cookies.Delete("AuthenticationFailureReason");
    }

       // <text>@f.Get(HttpContext?.User?.Identity?.Name)</text><br />
    
}
